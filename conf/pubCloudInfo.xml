<!--
 根据本配置文件扩展云类型
出现的类型则出现在账户和云类型里面
 -->
<?xml version="1.0" encoding="GB2312"?>
    <pubCloud>
        <name>ECS</name>
        <!-- 配置模板根据云厂商提供的方式进行填写。如云厂商没有提供模板，则InstanceModel为空。若提供，则分别填上，然后在agent端进行匹配转换为云商的模板 -->
        <InstanceModels>
            <InstanceModel>
                <CPU>1</CPU>
                <Image>ecs.sds.linux</Image>
                <Disk>1024</Disk>
            </InstanceModel>
            <InstanceModel>
                <CPU>2</CPU>
                <Image>ecs.sds.linux</Image>
                <Disk>2048</Disk>
            </InstanceModel>
            <InstanceModel>
                <CPU>3</CPU>
                <Image>ecs.sds.linux</Image>
                <Disk>4096</Disk>
            </InstanceModel>
        </InstanceModels>
        <!-- 在前端，是所有身份信息的并集。此处定义此云厂商定义身份所需要的最小集，在java处理时，只把此项传到agent -->
        <AccountInfo>
           <value>accessKeyID</value>
           <value>accessKeyKey</value>
        </AccountInfo>
    </pubCloud>
    <pubCloud>
     <name>EC2</name>
        <!-- 配置模板根据云厂商提供的方式进行填写。如云厂商没有提供模板，则InstanceModel为空。若提供，则分别填上，然后在agent端进行匹配转换为云商的模板 -->
        <InstanceModels>
        </InstanceModels>
        <!-- 在前端，是所有身份信息的并集。此处定义此云厂商定义身份所需要的最小集，在java处理时，只把此项传到agent -->
        <AccountInfo>
           <value>token</value>
           <value></value>
        </AccountInfo>
    </pubCloud>
    <pubCloud>
    </pubCloud>
</xml>